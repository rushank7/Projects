    "def rotate(arr,n):\n",
    "    temp = [0]*n\n",
    "    for i in range(1,n):\n",
    "        temp[i -1] = arr[i]\n",
    "    temp[n - 1] = arr[0]\n",
    "    print(temp)"
   ]
